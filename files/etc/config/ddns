
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'f3322'
	option ip_source 'network'
	option service_name '3322.org'
	option lookup_host 'aromalct.f3322.net'
	option domain 'aromalct.f3322.net'
	option password '12345678'
	option username 'root'
	option enabled '1'
	option interface 'vwan1'
	option ip_network 'vwan1'

config service 'noip'
	option service_name 'no-ip.com'
	option lookup_host 'aroma159.ddns.net'
	option domain 'aroma159.ddns.net'
	option username 'aromalct'
	option password 'Aromalct1982'
	option enabled '1'
	option interface 'vwan2'
	option ip_network 'vwan2'

config service 'dynu'
	option service_name 'dynu.com'
	option lookup_host 'aromalct.ddnsfree.com'
	option domain 'aromalct.ddnsfree.com'
	option username 'Aromali'
	option password 'Aromalct1982'
