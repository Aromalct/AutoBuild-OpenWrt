# ========================固件定制部分========================

CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_x86_64_Generic=y
CONFIG_PACKAGE_grub2-efi=y
CONFIG_EFI_IMAGES=y
CONFIG_TARGET_KERNEL_PARTSIZE=16
CONFIG_TARGET_ROOTFS_PARTSIZE=300

CONFIG_PACKAGE_curl=y
CONFIG_PACKAGE_nano=y
CONFIG_PACKAGE_wget=y

# 固件压缩:
# CONFIG_TARGET_IMAGES_GZIP is not set

# 取消编译VMware镜像
# CONFIG_VMDK_IMAGES is not set

# sftp服务支持
CONFIG_PACKAGE_openssh-sftp-server=y

# 释放内存
CONFIG_PACKAGE_luci-app-ramfree=y
CONFIG_PACKAGE_luci-i18n-ramfree-zh-cn=y

# Web管理
# CONFIG_PACKAGE_luci-app-webadmin is not set
# CONFIG_PACKAGE_luci-i18n-webadmin-zh-cn is not set

# TTYD终端
CONFIG_PACKAGE_luci-app-ttyd=y
CONFIG_PACKAGE_luci-i18n-ttyd-zh-cn=y
CONFIG_PACKAGE_ttyd=y

# 时间同步
CONFIG_PACKAGE_luci-app-ntpc=y
CONFIG_PACKAGE_luci-i18n-ntpc-zh-cn=y
CONFIG_PACKAGE_ntpclient=y

# 文件传输
CONFIG_PACKAGE_luci-app-filetransfer=y

# 上网时间控制
# CONFIG_PACKAGE_luci-app-accesscontrol is not set

# 动态DDNS
CONFIG_PACKAGE_ddns-scripts_cloudflare.com-v4=y
CONFIG_PACKAGE_ddns-scripts_cnkuai_cn is not set
CONFIG_PACKAGE_ddns-scripts_freedns_42_pl=y
CONFIG_PACKAGE_ddns-scripts_godaddy.com-v1=y
CONFIG_PACKAGE_ddns-scripts_no-ip_com=yntpc
CONFIG_PACKAGE_ddns-scripts_nsupdate=y
CONFIG_PACKAGE_ddns-scripts_route53-v1=y

# 解锁网易云灰色歌曲
CONFIG_PACKAGE_luci-app-unblockmusic=y
# CONFIG_PACKAGE_luci-app-unblockneteasemusic-mini is not set

# SSR plus:
CONFIG_PACKAGE_luci-app-ssr-plus=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Shadowsocks=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Simple_obfs=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_V2ray_plugin=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_V2ray=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Trojan=y
# CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Redsocks2 is not set
# CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Kcptun is not set
# CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_ShadowsocksR_Server is not set
# CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_ShadowsocksR_Socks is not set

# 百度网盘管理
# CONFIG_PACKAGE_luci-app-baidupcs-web is not set
# CONFIG_PACKAGE_baidupcs-web is not set

# Docker容器
# CONFIG_PACKAGE_luci-app-docker is not set
# CONFIG_PACKAGE_docker-ce is not set

# SmartDNS
# CONFIG_PACKAGE_luci-app-smartdns is not set
# CONFIG_PACKAGE_luci-i18n-smartdns-zh-cn is not set
# CONFIG_PACKAGE_smartdns is not set

# 断网检测功能与定时重启 watchcat
# CONFIG_PACKAGE_luci-app-watchcat is not set
# CONFIG_PACKAGE_luci-i18n-watchcat-zh-cn is not set
# CONFIG_PACKAGE_watchcat is not set

# 网络唤醒
# CONFIG_PACKAGE_luci-app-wol is not set

# KMS服务器
CONFIG_PACKAGE_luci-app-vlmcsd=y
CONFIG_PACKAGE_vlmcsd=y

# 迅雷快鸟
# CONFIG_PACKAGE_luci-app-xlnetacc is not set

# BT下载工具（qBittorrent）
